insert into snsboard values(seq_board.nextval,'최진평','시발 맥북','부셔버리고 싶네',sysdate);
insert into snsboard values(seq_board.nextval,'최희찬','이거 못 품?','개쉬운데 ㅋㅋ',sysdate);
