package sist0622;

public class QuizForWhile_01 {
	
	//같은 이름의 메소드가 여러개 있어도 인자가 다르면 사용가능
	//=>오버로딩
	public static void quiz1(){
		//가로로 출력 _ for _ 1 2 3 4 5 그리고 다음 줄에 종료 라고 출력하시오
		for(int i=1;i<=5;i++)
			System.out.print(i+" \n");
		System.out.print("종료");
		
	}
	
