package sist0621;

public class ExFor_07 {

	public static void main(String[] args) {

		//초기값 조건 안맞으면 한 번도 실행안
		for(int i=5;i<=1;i++)
			System.out.println(i);
		
		int i;
		for(i=1;i<=10;i++)
			System.out.print(i+" ");
		System.out.println("\n빠져 나온 후의 i값: "+i); //변수 앞에 뺐을 경우 사용 가능
		
		int a=10;
		for(;a<=50;a+=5)
			System.out.print(a+" ");
		System.out.println("\n빠져 나온 후의 a값: "+a);
		
	}

}
