		/*for(int dan=2;dan<=9;dan++)
		{
			for(int j=1;j<=9;j++)
			{
				System.out.println(dan+"x"+j+"="+dan*j);
			}
			
			System.out.println();
		}*/
		
		
		//구구단으로 break연습
		
		for(int dan=2;dan<=9;dan++)
		{
			for(int j=1;j<=9;j++)
			{
				System.out.println(dan+"x"+j+"="+dan*j);
			}
			System.out.println();
		}

	}

}
