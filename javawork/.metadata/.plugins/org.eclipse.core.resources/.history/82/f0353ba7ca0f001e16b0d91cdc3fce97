package sist0621;

import java.util.Scanner;

public class ExWhile_02 {

	public static void main(String[] args) {
		//while문으로 1부터 10까지출력
		
		int n=0;
		
		while(n<10)
		{
			n++;
			System.out.printf("%4d",n);
		}
		
		System.out.println();
		
		//다른 방법
		
		n=0; //위에 이미 int n 선언했기에 n을 다시 초기화
		
		while(n<10)
		{
			System.out.printf("%4d",n++);
		}
	}

}
