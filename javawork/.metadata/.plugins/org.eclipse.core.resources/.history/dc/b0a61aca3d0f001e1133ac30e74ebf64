package sist0620;

public class ForTest_16 {

	public static void main(String[] args) {
		// 1. 1~5까지 출력하시오
		
		
		   //int i는 지역변수
		   //1개의 for문을 벗어나면 다시 선
		for(int i=1;i<=5;i++)
		{
			System.out.print(i+" ");
		}
		
		System.out.println(); //첫번째 for문 후 줄 내리는 기능
		
		for(int i=5;i>=1;i--)
		{
			System.out.print(i+" ");
		}
		
		System.out.println();
		
		//0,2,4,6,8,10
		for(int i=0;i<=10;i+=2)
		{
			System.out.print(i+" ");
		}

	}

}
