package day0627; //메모리에 할당이 되더라도 프로그램 종료시 해제

public class MemoryCnt_03 {
	
	//int count=0;
	
	static int count=0;
	
	//생성자명은 클래스명과 똑같음
	public MemoryCnt_03() {
		count++;
		System.out.println(count);
	}
	
	
	

	public static void main(String[] args) {

		
		MemoryCnt_03 mem1=new MemoryCnt_03();
		MemoryCnt_03 mem2=new MemoryCnt_03();
	}

}
