package sist0621;

public class QuizFor_09 {

	public static void main(String[] args) {

		//Q.1 2 4 5 7 8 10 _continue이용해서 출력
		
		for(int i=1;i<=10;i++)
		{
			if(i%3==0)
				continue;
			System.out.print(i+" ");
		}
		System.out.println();
		
		/*Q. continue이용할것!!
		 * 홀수값: 1
		 * 홀수값: 3
		 * 홀수값; 5
		 * 홀수값: 7
		 * 홀수값: 9 */
		
		for(int i=1;i<=9;i++)
		{
			if(i%2==0)
				continue;
			System.out.println("홀수값: "+i);
		}
		
	}

}
