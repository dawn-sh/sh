package sist0622;

import java.util.Scanner;

public class GaroGugu_06 {

	public static void main(String[] args) {
		//가로방향으로 구구단을 출력하세요
		//[2단] [3단] ...... [9단]
		int dan;
		
		for(int i=2;i<=9;i++)
		{
			for(dan=2;dan<=9;dan++)
			{
				System.out.println("["+dan+"]");
			}
			System.out.print(dan+"x"+i+"="+dan*i);
		}

	}

}
