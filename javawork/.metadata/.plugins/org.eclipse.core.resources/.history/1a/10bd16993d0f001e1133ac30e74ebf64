package sist0620;

public class ForTest_16 {

	public static void main(String[] args) {
		// 1. 1~5까지 출력하시오
		
		
		   //int i 지역변수
		   //1개의 for문을 벗어나면 다시 선
		for(int i=1;i<=5;i++)
		{
			System.out.print(i+" ");
		}
		
		System.out.println(); //첫번째 for문 후 줄 내리는 기
		for(int i=5;i>=1;i--)
		{
			System.out.print(i+" ");
		}

	}

}
